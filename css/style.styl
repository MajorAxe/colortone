 $color-blue = #00a6ff
 $color-light-blue = #60bce5
 $color-grey = #373435
 $color-grey-333 = #373435
 $color-greed-dark = #044
 $GB = Gilroy-ExtraBold
 $GL = Gilroy-Light
 @font-face 
    font-family Gilroy-ExtraBold /* Имя шрифта */
    src url(../fonts/Gilroy-ExtraBold.otf) /* Путь к файлу со шрифтом */
    font-weight 900
   
 @font-face 
    font-family Gilroy-Light /* Имя шрифта */
    src url(../fonts/Gilroy-Light.otf) /* Путь к файлу со шрифтом */
    font-weight 100
.mt80
    margin-top 80px
.pos-absolute
    position absolute
.pos-relative
    position: relative
.motion
    animation: kenBurns 15s ease alternate infinite
    -webkit-animation: kenBurns 15s ease alternate infinite
a.button
    font-family $GB
    padding-top 10px
    padding-bottom 10px
    font-size 20px
    color  black
    border 2px solid black
    border-radius 5px
    text-transform uppercase
    display inline-block
    transition 0.1s linear
    -webkit-transition 0.1s linear
    &:hover
        text-decoration none
        transform translate3d(0, -2px, 0)
        -webkit-transform translate3d(0, -2px, 0)
input.button
    transition 0.1s linear
    -webkit-transition 0.1s linear
    &:hover
        text-decoration none
        transform translate3d(0, -2px, 0)
        -webkit-transform translate3d(0, -2px, 0)
